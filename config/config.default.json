{
  "ui": {
    "icons": { "size": 38, "size_single_module": 32, "image_size": 150 },
    "orientation": "vertical",
    "ignore_exclusive": true,
    "fullscreen": false,
    "horizontal": "center",
    "vertical": "start",
    "width": 400,
    "margins": {
      "top": 100
    },
    "anchors": {
      "top": true
    }
  },
  "list": {
    "always_show": true,
    "fixed_height": false,
    "height": 300,
    "margin_top": 10,
    "max_entries": 50,
    "scrollbar_policy": "automatic",
    "show_initial_entries": true,
    "show_sub_single_module": false,
    "width": 400
  },
  "search": {
    "force_keyboard_focus": true,
    "icons": true,
    "history": true,
    "placeholder": "Search...",
    "delay": 0
  },
  "activation_mode": {},
  "builtins": {
    "applications": { "actions": true, "prioritize_new": true },
    "clipboard": {
      "image_height": 300,
      "max_entries": 10,
      "switcher_only": true
    },
    "commands": { "switcher_only": true },
    "custom_commands": {},
    "emojis": { "switcher_only": true, "history": true, "typeahead": true },
    "finder": { "switcher_only": true, "ignore_gitignore": true },
    "hyprland": { "context_aware_history": true },
    "runner": { "typeahead": true, "history": true, "generic_entry": false },
    "ssh": { "switcher_only": true, "history": true },
    "switcher": { "prefix": "/" },
    "websearch": { "engines": ["google"] },
    "dmenu": {}
  }
}
