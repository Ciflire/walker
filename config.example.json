{
  "terminal": "foot",
  "placeholder": "Search...",
  "shell_config": "/home/andrej/.zshrc",
  "keep_open": false,
  "notify_on_fail": true,
  "show_initial_entries": true,
  "fullscreen": true,
  "disable_activation_mode": false,
  "search": {
    "delay": 150, // in ms
    "hide_icons": true
  },
  "align": {
    "width": 400,
    "horizontal": "center",
    "vertical": "start",
    "margins": {
      "top": 300,
      "bottom": 0,
      "end": 0,
      "start": 0
    }
  },
  "list": {
    "height": 300,
    "style": "fixed",
    "always_show": true
  },
  "orientation": "vertical",
  "icons": {
    "hide": false,
    "size": 38,
    "image_height": 200
  },
  "modules": [
    {
      "name": "runner",
      "prefix": ""
    },
    {
      "name": "applications",
      "prefix": ""
    },
    {
      "name": "websearch",
      "prefix": "?"
    }
  ]
}
